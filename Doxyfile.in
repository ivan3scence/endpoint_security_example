PROJECT_NAME           = "@CMAKE_PROJECT_NAME@"
PROJECT_NUMBER         = "@PROJECT_VERSION@"
PROJECT_BRIEF          = "Endpoint Security Framework example"

INPUT                  = @CMAKE_CURRENT_SOURCE_DIR@ @CMAKE_CURRENT_SOURCE_DIR@/README.md
OUTPUT_DIRECTORY       = @CMAKE_CURRENT_BINARY_DIR@/docs
RECURSIVE              = YES

EXTRACT_ALL            = YES
EXTRACT_PRIVATE        = YES
EXTRACT_STATIC         = YES

GENERATE_HTML          = YES
GENERATE_LATEX         = NO

FILE_PATTERNS          = *.cpp *.h *.md
EXCLUDE_PATTERNS       = */build/* */.git/*

QUIET                  = YES
WARNINGS               = YES
WARN_IF_UNDOCUMENTED   = YES
